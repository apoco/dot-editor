{
  "name": "dot-editor",
  "version": "0.1.0",
  "description": "Easily edit & preview graphs written in the DOT language",
  "keywords": [
    "DOT",
    "graphviz",
    "dag",
    "graph"
  ],
  "license": "GPL-3.0",
  "homepage": "https://github.com/apoco/dot-editor#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/apoco/dot-editor.git"
  },
  "bugs": {
    "url": "https://github.com/apoco/dot-editor/issues"
  },
  "author": "Jacob Page <jacob.h.page@gmail.com>",
  "main": "./lib/main",
  "dependencies": {
    "electron-store": "^2.0.0",
    "react": "^16.4.1",
    "react-ace": "^6.1.4",
    "react-dom": "^16.4.1",
    "rxjs": "^6.2.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron": "^2.0.5",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm-run-all --parallel --race watch:babel start",
    "clean": "npm run clean:babel",
    "prebuild": "npm run clean",
    "build": "npm run build:babel",
    "start": "npm run build && npm run start:electron",
    "clean:babel": "rimraf ./lib",
    "build:babel": "babel --copy-files --out-dir ./lib ./src",
    "watch:babel": "babel --watch --copy-files --out-dir ./lib ./src",
    "start:electron": "electron ."
  }
}
